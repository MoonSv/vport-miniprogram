/* pages/couses.wxss */

.page-body{
  padding: 10px;
  background-color: #f8f8f8;
  height: 100%;
}
.page-title{
  font-size: 22px;
  color: rgb(90, 90, 90);
  display: inline;
  padding-bottom: 5px;
  border-bottom: 2px solid grey
}
.artile-list{
  border: 1px solid #d8d8d8;
  border-radius: 5px;
  box-shadow: 0 2px 5px #efefef;
  padding: 20px;
  margin-top: 13px;
  height: 120rpx;
}
.artile-list.level1{
  background-color: rgba(161, 236, 91, 0.397)
}
.artile-list.level2{
  background-color: rgba(255, 223, 79, 0.329)
}
.artile-list.level3{
  background-color: rgba(137, 208, 255, 0.397)
}
.artile-list.level4{
  background-color: rgba(176, 111, 230, 0.397)
}
.pre-info{
  color: rgb(104, 104, 104);
}
.couse-title{
  font-size: 18px;
  color: rgb(73, 73, 73);
}
.couse-date{
  /* margin-left: 20px; */
  font-size: 12px;
  color: rgb(109, 109, 109);
}